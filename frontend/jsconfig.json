{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/components/layouts/*"],
      "@errors/*": ["src/components/Errors/*"],
      "@config/*": ["src/config/*"],
      "@store/*": ["src/store/*"],
      "@data/*": ["src/data/*"],
      "@helpers/*": ["src/helpers/*"],
      "@images/*": ["src/app/assets/images/*"],
      "@public/*": ["public/*"]
    }
  },
  "include": ["src"]
}
